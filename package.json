    "daisyui": "^4.12.14",
    "framer-motion": "^11.13.1",
    "geist": "^1.3.0",
    "lucide-react": "^0.462.0",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.5",
