    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "translate": "pnpm run lingui:extract && node ./src/scripts/translate-locale.cjs",
    "db:generate": "drizzle-kit generate",
